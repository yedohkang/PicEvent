{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["apis/", "lib/" ],
    "exclude": [
      "lib/apirequest.js",
      "lib/generator.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "destination": "./doc/",
    "recurse": true,
    "verbose": true,
    "readme": "./README.md",
    "package": "./package.json"
  },
  "templates": {
    "theme": "lumen",
    "copyright": "google-api-nodejs-client Copyright Â© 2012-2016 Google Inc.",
    "outputSourceFiles": false,
    "linenums": true
  }
}
